{
    "name": "floorbox/event-tracker",
    "description": "An event tracker for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Fa Perreault",
        "email": "fa.perreault@thefloorbox.ca"
      }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
